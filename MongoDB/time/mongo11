#!/bin/bash
mongo localhost:27017/time --eval 'db.time.aggregate([{"$match":{"$and":[{"t_hour":12},{"t_minute":{"$gte":30}}]}},{"$unwind": "$store_sales"},{"$match":{"$and":[{"store_sales.customer.hd_dep_count":3},{"store_sales.s_store_name":"ese"}]}},{"$group":{"_id":null, "h12_30_to_13":{"$sum":1}}}]).toArray()' >>~/Documentos/benchmarktpc/mongoDB/time/resultados/query11.txt