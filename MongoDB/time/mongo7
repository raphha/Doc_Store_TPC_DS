#!/bin/bash
mongo localhost:27017/time --eval 'db.time.aggregate([{"$match":{"$and":[{"t_meal_time":{"$in":["breakfast","dinner"]}},{"store_sales.item.i_manager_id":1},{"store_sales.date.d_moy":11},{"store_sales.date.d_year":2001}]}},{"$unwind": "$store_sales"},{"$match":{"$and":[{"t_meal_time":{"$in":["breakfast","dinner"]}},{"store_sales.item.i_manager_id":1},{"store_sales.date.d_moy":11},{"store_sales.date.d_year":2001}]}},{"$project":{"store_sales.item.i_brand":1, "store_sales.item.i_brand_id":1, "t_hour":1, "t_minute":1, "store_sales.ss_ext_sales_price":1}},{$group:{"_id":{"i_brand": "$store_sales.item.i_brand","i_brand_id":"$store_sales.item.i_brand_id","t_hour":"$t_hour","t_minute":"$t_minute"}, "ext_price":{"$sum":"$store_sales.ss_ext_sales_price"}}}]).toArray()' >>~/Documentos/benchmarktpc/mongoDB/time/resultados/query7.txt