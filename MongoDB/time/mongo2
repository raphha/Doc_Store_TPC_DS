#!/bin/bash
mongo localhost:27017/time --eval 'db.time.aggregate([{"$match":{"$and":[{"store_sales.item.i_current_price":{"$gt":9}},{"store_sales.date.d_year":2001}]}},{"$unwind": "$store_sales"},{"$match":{"$and":[{"store_sales.item.i_current_price":{"$gt":9}},{"store_sales.date.d_year":2001}]}},{"$project":{"store_sales.customer.ca_state":1}},{"$group":{"_id":"$store_sales.customer.ca_state", "cnt":{"$sum":1}}}]).toArray()' >>~/Documentos/benchmarktpc/mongoDB/time/resultados/query2nova.txt