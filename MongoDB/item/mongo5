#!/bin/bash
mongo localhost:27017/item --eval 'db.item.aggregate([{$match:{$and:[{"i_category":{"$in":["Music","Home","Sports"]}},{"store_sales.date.d_month_seq":{"$gte":1200,"$lte":1211}}]}},{$unwind: "$store_sales"},{$match:{$and:[{"i_category":{"$in":["Music","Home","Sports"]}},{"store_sales.date.d_month_seq":{"$gte":1200,"$lte":1211}}]}},{$project:{"i_item_id":1, "i_item_desc":1, "i_category":1,"i_class":1,"i_current_price":1, "store_sales.ss_ext_sales_price":1}},{$group:{"_id":{"i_item_id":"$i_item_id", "i_item_desc":"$i_item_desc", "i_category":"$i_category","i_class":"$i_class","i_current_price":"$i_current_price"}, itemrevenue:{$sum:"$store_sales.ss_ext_sales_price"}}}]).toArray()' >>~/Documentos/benchmarktpc/mongoDB/item/resultados/query5nova.txt