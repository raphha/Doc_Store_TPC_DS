#!/bin/bash
mongo localhost:27017/store_sales --eval 'db.store_sales.aggregate([{"$match":{"$and":[{"item.i_manager_id":1},{"date.d_moy":11},{"date.d_year":2001},{"time.t_meal_time":{"$in":["breakfast","dinner"]}}]}},{"$project":{"item.i_brand":1, "item.i_brand_id":1, "time.t_hour":1, "time.t_minute":1, "ss_ext_sales_price":1}},{$group:{"_id":{"i_brand": "$item.i_brand","i_brand_id":"$item.i_brand_id","t_hour":"$time.t_hour","t_minute":"$time.t_minute"}, "ext_price":{"$sum":"$ss_ext_sales_price"}}}]).toArray()' >>~/Documentos/benchmarktpc/mongoDB/store_sales/resultados/query7.txt