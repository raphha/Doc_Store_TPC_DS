#!/bin/bash
mongo localhost:27017/store_sales --eval 'db.store_sales.aggregate([{$match:{$and:[{"item.i_category":{"$in":["Music","Home","Sports"]}},{"date.d_date":{"$gte":"2002-01-01","$lte":"2002-01-31"}}]}},{$project:{"item.i_item_id":1, "item.i_item_desc":1, "item.i_category":1,"item.i_class":1, "ss_ext_sales_price":1}},{$group:{"_id":{"i_item_id":"$item.i_item_id", "i_item_desc":"$item.i_item_desc", "i_category":"$item.i_category","i_class":"$item.i_class"}, itemrevenue:{$sum:"$ss_ext_sales_price"}}}]).toArray()' >>~/Documentos/benchmarktpc/mongoDB/store_sales/resultados/query12.txt