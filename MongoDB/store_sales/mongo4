#!/bin/bash
mongo localhost:27017/store_sales --eval 'db.store_sales.aggregate([{"$match":{"$and":[{"customer.cd_gender":"F"},{"customer.cd_marital_status":"S"},{"customer.cd_education_status":"Secondary"},{"date.d_year":2001},{"s_state":"TN"}]}},{"$project":{"item.i_item_id":1, "s_state":1, "ss_sales_price":1}},{$group:{"_id":{"i_item_id": "$item.i_item_id","s_state":"$s_state"}, "agg":{"$sum":"$ss_sales_price"}}},{"$limit":100}]).toArray()' >>~/Documentos/benchmarktpc/mongoDB/store_sales/resultados/query4.txt