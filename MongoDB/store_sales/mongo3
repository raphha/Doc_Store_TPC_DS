#!/bin/bash
mongo localhost:27017/store_sales --eval 'db.store_sales.aggregate([{"$match":{"$and":[{"item.i_manager_id":20},{"date.d_moy":11},{"date.d_year":2001},{"customer.ca_zip":"35709"}]}},{"$project":{"item.i_brand_id":1, "item.i_brand":1, "item.i_manufact_id":1, "item.i_manufact":1, "ss_ext_sales_price":1}},{$group:{"_id":{"i_brand_id": "$item.i_brand_id","i_brand":"$item.i_brand","i_manufact_id":"$item.i_manufact_id","i_manufact":"$item.i_manufact"}, "ext_price":{"$sum":"$ss_ext_sales_price"}}}]).toArray()' >>~/Documentos/benchmarktpc/mongoDB/store_sales/resultados/query3.txt