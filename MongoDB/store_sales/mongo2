#!/bin/bash
mongo localhost:27017/store_sales --eval 'db.store_sales.aggregate([{"$match":{"$and":[{"item.i_current_price":{"$gt":9}},{"date.d_year":2001}]}},{"$project":{"customer.ca_state":1}},{"$group":{"_id":"$customer.ca_state", "cnt":{"$sum":1}}}]).toArray()' >>~/Documentos/benchmarktpc/mongoDB/store_sales/resultados/query2nova.txt