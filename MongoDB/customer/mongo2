#!/bin/bash
mongo localhost:27017/customer --eval 'db.customer.aggregate([{"$match":{"$and":[{"store_sales.item.i_current_price":{"$gt":9}},{"store_sales.date.d_year":2001}]}},{"$unwind": "$store_sales"},{"$match":{"$and":[{"store_sales.item.i_current_price":{"$gt":9}},{"store_sales.date.d_year":2001}]}},{"$project":{"ca_state":1}},{"$group":{"_id":"$ca_state", "cnt":{"$sum":1}}}]).toArray()' >>~/Documentos/benchmarktpc/mongoDB/customer/resultados/query2nova.txt