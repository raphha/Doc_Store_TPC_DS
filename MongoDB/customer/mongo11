#!/bin/bash
mongo localhost:27017/customer --eval 'db.customer.aggregate([{"$match":{"$and":[{"hd_dep_count":3},{"store_sales.time.t_hour":12},{"store_sales.time.t_minute":{"$gte":30}},{"store_sales.s_store_name":"ese"}]}},{"$unwind": "$store_sales"},{"$match":{"$and":[{"hd_dep_count":3},{"store_sales.time.t_hour":12},{"store_sales.time.t_minute":{"$gte":30}},{"store_sales.s_store_name":"ese"}]}},{"$group":{"_id":null, "h12_30_to_13":{"$sum":1}}}]).toArray()' >>~/Documentos/benchmarktpc/mongoDB/customer/resultados/query11.txt