#!/bin/bash
mongo localhost:27017/customer --eval 'db.customer.aggregate([{$match:{$and:[{"store_sales.item.i_category":{"$in":["Music","Home","Sports"]}},{"store_sales.date.d_date":{"$gte":"2002-01-01","$lte":"2002-01-31"}}]}},{$unwind: "$store_sales"},{$match:{$and:[{"store_sales.item.i_category":{"$in":["Music","Home","Sports"]}},{"store_sales.date.d_date":{"$gte":"2002-01-01","$lte":"2002-01-31"}}]}},{$project:{"store_sales.item.i_item_id":1, "store_sales.item.i_item_desc":1, "store_sales.item.i_category":1,"store_sales.item.i_class":1, "store_sales.ss_ext_sales_price":1}},{$group:{"_id":{"i_item_id":"$store_sales.item.i_item_id", "i_item_desc":"$store_sales.item.i_item_desc", "i_category":"$store_sales.item.i_category","i_class":"$store_sales.item.i_class"}, itemrevenue:{$sum:"$store_sales.ss_ext_sales_price"}}}]).toArray()' >>~/Documentos/benchmarktpc/mongoDB/customer/resultados/query12.txt