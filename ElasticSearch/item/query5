#!/bin/bash
curl -XPOST 'localhost:9200/i_item/item/_search?pretty' -d '
{
  "query": {"filtered": {"filter": {
    "bool": {
	      "must": [ 
				 {"terms": {"i_category":["Music","Home","Sports"]}}, 
		{
    "filtered": {
      "filter": {
        "nested": {
          "inner_hits": {},
          "path": "store_sales",
          "query": {
            "filtered": {
              "filter": {
                "bool": {
                "must": [ 
                  {"range" : {"store_sales.date.d_month_seq":{"gte":1200, "lte":1211}}}
                ]
               }
              }
            }
          }
        }
      }
    }
   }]}}}},
          "aggs": {
   		 "group_by_item_id": {
     		 "terms": {
      		  "field": "i_item_id",
            "order" : { "_term" : "asc" }, "size": 10000
            },
            "aggs": {
   		 	"group_by_desc": {
     		 	"terms": {
      		 	 "field": "i_item_desc",
            "order" : { "_term" : "asc" }, "size": 10000
      		         },
					  "aggs": {
   		 	"group_by_category": {
     		 	"terms": {
      		 	 "field": "i_category",
            "order" : { "_term" : "asc" }, "size": 10000
      		         },
					 "aggs": {
   		 	"group_by_class": {
     		 	"terms": {
      		 	 "field": "i_class",
            "order" : { "_term" : "asc" }, "size": 100000
      		         },
					 "aggs": {
   		 	"group_by_price": {
     		 	"terms": {
      		 	 "field": "i_current_price",
            "order" : { "_term" : "asc" }, "size": 10000
      		         },
   "aggs": {
    "lineorder": {
      "nested": {
        "path": "store_sales"
      },
      "aggs": {
	  "only_loc": {
          "filter": {
                "bool": {
                "must": [ 
                  {"range" : {"store_sales.date.d_month_seq":{"gte":1200, "lte":1211}}}
                ]
               }
			   },
			"aggs": {
    			"itemrevenue": {
    			  "sum": {
     		        "field": "store_sales.ss_ext_sales_price"
         }}}}}}}}}
       }
     }
   }
  }
        }
		}
  }
}, "size":0
}' -o /home/raphael/Documentos/benchmarktpc/elastic/item/resultados/query5.txt
